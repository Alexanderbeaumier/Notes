{
  "family": "note-taking-app",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::314282564487:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::314282564487:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "notes-container",
      "image": "314282564487.dkr.ecr.us-east-1.amazonaws.com/notes:latest",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ]
    }
  ]
}
